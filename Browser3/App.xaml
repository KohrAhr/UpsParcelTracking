<Application x:Class="Browser3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Browser3"
             Startup="AppStartup">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Styles.xaml"/>

                <!-- Last resources - is default resource (language) -->
                <ResourceDictionary Source="Resources\StringResources.en-US.xaml" />
                <ResourceDictionary Source="Resources\StringResources.QuickSearch.en-US.xaml" />
                <ResourceDictionary Source="Resources\StringResources.DateSelectorBar.en-US.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
